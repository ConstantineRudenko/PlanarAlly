{
    "name": "client",
    "version": "0.29.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "clean": "rimraf ../server/static/vite/",
        "build:prep": "npm run clean",
        "build": "npm run build:prep && vite build && mv ../server/index.html ../server/templates/index.html",
        "build:ci": "npm run build:prep && npm run vue-tsc && vite build --outDir dist",
        "lint": "eslint src --fix",
        "lint:ci": "CI=1 eslint src --no-fix",
        "vue-tsc": "vue-tsc --noEmit",
        "tsc": "tsc --noEmit",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@babylonjs/materials": "^4.2.2",
        "@fortawesome/fontawesome-svg-core": "^6.1.0",
        "@fortawesome/free-brands-svg-icons": "^6.1.0",
        "@fortawesome/free-regular-svg-icons": "^6.1.0",
        "@fortawesome/free-solid-svg-icons": "^6.1.0",
        "@fortawesome/vue-fontawesome": "^3.0.0-5",
        "@planarally/dice": "^0.4.0-alpha.7",
        "core-js": "^3.21.1",
        "path-data-polyfill": "^1.0.3",
        "socket.io-client": "^4.4.1",
        "swiper": "^7.3.1",
        "tinycolor2": "^1.4.2",
        "vue": "^3.2.31",
        "vue-i18n": "^9.1.9",
        "vue-router": "^4.0.14",
        "vue-toastification": "^2.0.0-rc.5",
        "vue3-markdown-it": "^1.0.10",
        "vuedraggable": "4.0.3"
    },
    "devDependencies": {
        "@babylonjs/core": "^4.2.2",
        "@intlify/vite-plugin-vue-i18n": "^3.3.1",
        "@types/cannon": "^0.1.8",
        "@types/lodash": "^4.14.180",
        "@types/swiper": "^5.4.3",
        "@types/tinycolor2": "^1.4.3",
        "@typescript-eslint/eslint-plugin": "^5.15.0",
        "@typescript-eslint/parser": "^5.15.0",
        "@vitejs/plugin-vue": "^2.2.4",
        "c8": "^7.11.0",
        "eslint": "^8.11.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.5.0",
        "happy-dom": "^2.47.2",
        "prettier": "^2.6.0",
        "rimraf": "^3.0.2",
        "rollup-plugin-visualizer": "^5.6.0",
        "sass": "^1.49.9",
        "sass-loader": "^12.6.0",
        "typescript": "^4.6.2",
        "upath": "^2.0.1",
        "vite": "^2.8.6",
        "vitest": "^0.6.3",
        "vue-tsc": "^0.33.2"
    },
    "madge": {
        "detectiveOptions": {
            "ts": {
                "skipTypeImports": true
            }
        }
    }
}
