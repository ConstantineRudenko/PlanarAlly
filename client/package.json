{
    "name": "client",
    "version": "0.29.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "serve": "vite preview",
        "clean": "rimraf ../server/static/vite/",
        "build:prep": "npm run clean",
        "build": "npm run build:prep && vite build && mv ../server/index.html ../server/templates/index.html",
        "build:ci": "npm run build:prep && npm run tsc && vite build --outDir dist",
        "lint": "eslint src --fix",
        "lint:ci": "CI=1 eslint src --no-fix",
        "vue-tsc": "vue-tsc --noEmit",
        "tsc": "tsc --noEmit"
    },
    "dependencies": {
        "@babylonjs/materials": "^4.2.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/free-regular-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/vue-fontawesome": "^3.0.0-5",
        "@planarally/dice": "^0.4.0-alpha.7",
        "core-js": "^3.19.0",
        "path-data-polyfill": "^1.0.3",
        "socket.io-client": "^4.3.2",
        "swiper": "^7.3.1",
        "tinycolor2": "^1.4.2",
        "vue": "^3.2.26",
        "vue-i18n": "^9.1.9",
        "vue-router": "^4.0.12",
        "vue-toastification": "^2.0.0-rc.5",
        "vue3-markdown-it": "^1.0.9",
        "vuedraggable": "4.0.3"
    },
    "devDependencies": {
        "@babylonjs/core": "^4.2.0",
        "@intlify/vite-plugin-vue-i18n": "^2.5.0",
        "@types/cannon": "^0.1.7",
        "@types/lodash": "^4.14.176",
        "@types/swiper": "^5.4.3",
        "@types/tinycolor2": "^1.4.3",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "@vitejs/plugin-vue": "^1.10.1",
        "@vue/compiler-sfc": "^3.2.26",
        "eslint": "^8.1.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.0.3",
        "prettier": "^2.4.1",
        "rimraf": "^3.0.2",
        "sass": "^1.43.4",
        "sass-loader": "^12.3.0",
        "typescript": "~4.5.5",
        "upath": "^2.0.1",
        "vite": "^2.7.2",
        "vue-tsc": "^0.29.8"
    },
    "madge": {
        "detectiveOptions": {
            "ts": {
                "skipTypeImports": true
            }
        }
    }
}
